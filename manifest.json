{
    "manifest_version": 2,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "Description of your extension",
    "permissions": ["activeTab", "debugger", "storage", "contextMenus"],
    "background": {
        "service_worker": "background.js"
      },
    "browser_action": {
        "default_icon": "icon16.png"
    },
    "icons": {
      "16": "icon16.png"
    },
    "host_permissions": ["<all_urls>"]
  }